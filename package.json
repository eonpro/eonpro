{
  "name": "eonpro",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev -p ${PORT:-3001}",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "next lint && eslint . --ext .ts,.tsx",
    "lint:fix": "next lint --fix && eslint . --ext .ts,.tsx --fix",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate": "npm run type-check && npm run lint && npm run test:coverage",
    "validate:ci": "npm run type-check && npm run lint:strict && npm run test:coverage",
    "security:audit": "npm audit --production --audit-level=high",
    "security:check": "npm run security:audit && npm run lint:strict",
    "prepare": "husky install",
    "setup-fonts": "node scripts/setup-sofia-pro.js",
    "docker:build": "docker build -t eonpro:latest .",
    "docker:build:prod": "docker build -f infrastructure/docker/Dockerfile.production -t eonpro:prod .",
    "docker:run": "docker run -p 3000:3000 --env-file .env eonpro:latest",
    "docker:up": "docker-compose up -d",
    "docker:up:prod": "docker-compose -f infrastructure/docker/docker-compose.production.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "redis:start": "docker run -d -p 6379:6379 redis:7-alpine",
    "queue:worker": "tsx scripts/queue-worker.ts",
    "websocket:server": "tsx scripts/websocket-server.ts",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next node_modules coverage test-results"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.956.0",
