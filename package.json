{
  "name": "lifefile-prod",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT:-3001}",
    "build": "prisma generate && next build --webpack",
    "db:migrate": "prisma migrate deploy",
    "start": "next start",
    "postinstall": "prisma generate",
    "test": "vitest run --coverage",
    "lint": "next lint && eslint . --ext .ts,.tsx",
    "lint:fix": "next lint --fix && eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "setup-fonts": "node scripts/setup-sofia-pro.js",
    "docker:build": "docker build -t lifefile-ehr:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env lifefile-ehr:latest",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "redis:start": "docker run -d -p 6379:6379 redis:7-alpine",
    "queue:worker": "tsx scripts/queue-worker.ts",
    "websocket:server": "tsx scripts/websocket-server.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
